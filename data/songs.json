[{"album": "Stuck in the South", "review": "Raised as a Seventh-day Adventist in South Carolina, Adia Victoria eventually moved to Nashville where she has started making waves in the local music scene.", "title": "Stuck in the South", "song_art": "/assets/music/programs/asc/2014/08/adiavictoriacover_sq.jpg", "genre_tags": ["Folk / Americana"], "itunes": "https://itunes.apple.com/us/album/stuck-in-the-south/id902517003?i=902517121&uo=4", "ASIN": "B00M2CXAW4", "spotify": "http://open.spotify.com/album/5247Y7YI9Xsb8YStgx42DI", "rdio": "http://rd.io/x/QURQbDdcSw0X/", "reviewer": "Ann Powers", "artist": "Adia Victoria", "id": "1", "curator_tags": ["Ann Powers", "Lars Gotrich", "Tom Huizenga"], "media_url": "/npr/ascsongs/2014/08/20140805_ascsongs_03"}, {"album": "Ruins", "review": "For all the credit given to Harris' gorgeous voice and enveloping beds of sound, the first song on her new album\u00a0Ruins\u00a0highlights her talents as a lyricist.", "title": "Clearing", "song_art": "/assets/img/2014/11/20/grouper-ruins_sq.jpg", "genre_tags": ["Rock", "Unclassified"], "itunes": "https://itunes.apple.com/us/album/clearing/id909173677?i=909173682&uo=4", "ASIN": "B00MQJXBX6", "spotify": "http://open.spotify.com/track/3IL43TRkEnh4buGOZ1Brb5", "rdio": "http://rd.io/x/QURQbDdcfwRu/", "reviewer": "Otis Hart", "artist": "Grouper", "id": "2", "curator_tags": ["Otis Hart", "Robin Hilton", "Lars Gotrich"], "media_url": "/npr/fl/2014/10/20141021_fl_grouper02"}, {"album": "High Life", "review": "Their creative juices haven't abated in the slightest \u2014 \"DBF\" sounds like Eno's work with the Talking Heads crossed with\u00a0Fela Kuti.", "title": "DBF", "song_art": "/assets/img/2014/11/20/eno-hyde_sq.jpg", "genre_tags": ["Unclassified", "Rock"], "itunes": "https://itunes.apple.com/us/album/dbf/id882740748?i=882746541&uo=4", "ASIN": "B00L9JLSS4", "spotify": "http://open.spotify.com/track/7bBZrJo9aKHvS82ZK6Jofb", "rdio": "http://rd.io/x/QURQbDdcYkcQ/", "reviewer": "Jacob Ganz", "artist":